        <9f34bee1-9cff-4222-8697-06135d2e264d> ;
    skos:prefLabel "Newtonsche Gesetze"@de ;
    skos:relatedMatch <http://w3id.org/openeduhub/vocabs/discipline/460> ;
    sdo:keywords "Gesetze der Bewegung"@de,
        "Grundsätze der Bewegung"@de .

